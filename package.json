{
  "name": "guidant",
  "version": "0.1.0",
  "description": "AI Agent Workflow Orchestrator - Guide AI assistants through systematic development processes",
  "main": "index.js",
  "type": "module",
  "bin": {
    "guidant": "bin/guidant.js",
    "guidant-mcp": "mcp-server/server.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest tests/",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest tests/ --watch",
    "mcp-server": "node mcp-server/server.js",
    "dev": "node scripts/dev.js",
    "build": "echo 'Build complete'",
    "start": "node index.js",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write"
  },
  "keywords": [
    "ai",
    "workflow",
    "orchestration",
    "claude",
    "development",
    "automation",
    "mcp",
    "agent",
    "guidance"
  ],
  "author": "Guidant Team",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/perplexity": "^1.1.9",
    "@anthropic-ai/sdk": "^0.39.0",
    "@openrouter/ai-sdk-provider": "^0.7.1",
    "ai": "^4.3.16",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "fastmcp": "^1.20.5",
    "figlet": "^1.8.1",
    "fuse.js": "^7.1.0",
    "glob": "^11.0.2",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.1.0",
    "ora": "^8.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "execa": "^9.6.0",
    "ink": "^6.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mock-fs": "^5.5.0",
    "tsx": "^4.19.4"
  }
}
